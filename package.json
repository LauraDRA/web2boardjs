{
  "name": "web2boardjs",
  "version": "1.0.0",
  "description": "Application to communicate with Bitbloq to compile and load programs",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win": "electron-builder -w"
  },
  "author": "BQ",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.53.7",
    "electron-rebuild": "^1.6.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "avrgirl-arduino": "^2.2.7",
    "electron-log": "^2.2.12",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "socket.io": "^2.0.4",
    "uuid": "^3.1.0"
  },
  "build": {
    "appId": "com.bq.web2boardjs",
    "productName": "web2boardjs",
    "copyright": "Copyright Â© 2018 BQ",
    "directories": {
      "buildResources": "buildResources"
    },
    "files": [
      "main.js",
      "libs/*.*",
      "res/arduino_libs/*.*"
    ],
    "asar": true,
    "asarUnpack": [
      "res/arduino_libs"
    ],
    "protocols": {
      "name": "Bitbloq web2board launcher",
      "schemes": [
        "web2board"
      ]
    },
    "mac": {
      "files": [
        "res/arduino_ide/darwin",
        "res/drivers/darwin"
      ],
      "asarUnpack": [
        "res/arduino_ide/darwin",
        "res/drivers/darwin"
      ],
      "category": "public.app-category.education"
    },
    "win": {
      "files": [
        "res/arduino_ide/darwin",
        "res/drivers/darwin"
      ],
      "asarUnpack": [
        "res/arduino_ide/win32",
        "res/drivers/win32"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  }
}